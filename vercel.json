{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "styles.css",
            "use": "@vercel/static"
        },
        {
            "src": "script.js",
            "use": "@vercel/static"
        },
        {
            "src": "data.json",
            "use": "@vercel/static"
        },
        {
            "src": "background.png",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/analyze",
            "dest": "/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}